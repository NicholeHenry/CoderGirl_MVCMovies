@model CoderGirl_MVCMovies.Models.MovieRating

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>MovieRating</h4>

<form method='post'>

    <input readonly asp-for="MovieName" />

    <select asp-for="Rating">


        <option>1</option>
        <option>2</option>
        <option>3</option>
        <option>4</option>
        <option>5</option>
    </select>
    <button type='submit'>Update Rating</button>
    
</form>
